    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="14dp"
        android:layout_gravity="center"
        android:text="Stock Management"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="20dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/normal_spacing">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:columnCount="2"
            android:orientation="horizontal"
            android:useDefaultMargins="true">

            <LinearLayout
                android:id="@+id/uploadProduct"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_columnWeight="1"
                android:gravity="center"
                android:background="@drawable/admin_box"
                android:backgroundTint="@color/off_white"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:src="@drawable/add_button"
                    app:tint="@color/primaryColor" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:text="Upload Product"
                    android:textSize="14dp"
                    android:textColor="@color/black" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/productsLL"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_columnWeight="1"
                android:gravity="center"
                android:background="@drawable/admin_box"
                android:backgroundTint="@color/off_white"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:src="@drawable/products"
                    app:tint="@color/primaryColor" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:text="Products"
                    android:textSize="14dp"
                    android:textColor="@color/black" />


            </LinearLayout>




            <LinearLayout
                android:id="@+id/categoryList"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_columnWeight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:background="@drawable/admin_box"
                android:backgroundTint="@color/off_white">

                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:src="@drawable/category"
                    app:tint="@color/primaryColor" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:text="Categories"
                    android:textColor="@color/black"
                    android:textSize="14dp"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/addCategory"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_columnWeight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:background="@drawable/admin_box"
                android:backgroundTint="@color/off_white">


                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:src="@drawable/add_button"
                    app:tint="@color/primaryColor" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:text="Upload Category"
                    android:textSize="14dp"
                    android:textColor="@color/black" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/prescriptionsLL"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_columnWeight="1"
                android:gravity="center"
                android:background="@drawable/admin_box"
                android:backgroundTint="@color/white"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:src="@drawable/orders"
                    app:tint="@color/primaryColor" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:text="Prescriptions"
                    android:textSize="14dp"
                    android:textColor="@color/black" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_columnWeight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:background="@drawable/admin_box"
                android:backgroundTint="@color/white">

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:src="@drawable/ic_baseline_local_shipping_24"
                    app:tint="@color/primaryColor" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:text="Orders"
                    android:textSize="14dp"
                    android:textColor="@color/black" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/dashBoardLL"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_columnWeight="1"
                android:gravity="center"
                android:background="@drawable/admin_box"
                android:backgroundTint="@color/white"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:src="@drawable/orders"
                    app:tint="@color/primaryColor" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/small_spacing"
                    android:text="DashBoard"
                    android:textSize="14dp"
                    android:textColor="@color/black" />


            </LinearLayout>






